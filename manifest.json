{ 
    "name": "(TEST) Wishlist Cart", 
    "version": "1", 
    "description": "Global wishlist cart to store all the stuff you're longing for", 
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [ 
      { 
          "js": [ "wishlist.js" ],
           "matches": [ "http://*/*", "https://*/*"]
       }
   ],
    "browser_action": {
      "default_popup": "popup.html"
    },
     "manifest_version": 2
} 